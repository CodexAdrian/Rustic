{
  "schemaVersion": 1,
  "id": "rustic",
  "version": "${version}",
  "name": "Rustic",
  "description": "Rustic mod for the Rustic thingy in Minecraft 1.19.2",
  "authors": [
    "Alex Nijjar",
    "CodexAdrian",
    "Facu",
    "ThatGravyBoat"
  ],
  "contributors": [
    "Kekie6"
  ],
  "contact": {
    "website": "https://www.curseforge.com/minecraft/mc-mods/rustic",
    "repo": "https://github.com/terrarium-earth/Rustic",
    "issues": "https://github.com/terrarium-earth/Rustic/issues"
  },
  "license": "Terrarium Licence",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "earth.terrarium.rustic.fabric.RusticFabric",
      "earth.terrarium.rustic.Rustic::postInit"
    ],
    "client": [
      "earth.terrarium.rustic.client.fabric.RusticClientFabric"
    ],
    "modmenu": [
      "earth.terrarium.rustic.common.config.fabric.ModMenuConfig"
    ]
  },
  "accessWidener": "rustic.accesswidener",
  "mixins": [
    "rustic-common.mixins.json",
    "rustic.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": ">=1.19",
    "resourcefullib": ">=${resourcefullib_version}",
    "resourcefulconfig": ">=${resourcefulconfig_version}",
    "botarium": ">=${botarium_version}"
  }
}
